-- Initial ttstar.cabal generated by cabal init.  For further 
-- documentation, see http://haskell.org/cabal/users-guide/

name:                ttstar
version:             0.1.0.0
synopsis:            Dependent calculus with erasure inference
-- description:         
homepage:            http://github.com/ziman/ttstar
license:             BSD3
license-file:        LICENSE
author:              Matus Tejiscak
maintainer:          ziman@functor.sk
-- copyright:           
category:            Language
build-type:          Simple
extra-source-files:  README.md
cabal-version:       >=1.10

executable ttstar
  main-is:             Main.hs
  -- other-modules:       
  other-extensions:    FlexibleInstances, StandaloneDeriving
  build-depends:       base >=4.6 && <4.7, parsec >=3.1 && <3.2, containers >=0.5 && <0.6, transformers >=0.4 && <0.5
  -- hs-source-dirs:      
  default-language:    Haskell2010