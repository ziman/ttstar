-- vim: ft=ttstar

data Bool : Type where
  T : Bool,
  F : Bool.

data TwoBools : Type where
  TB : (x :R: Bool) -> (y :R: Bool) -> TwoBools.

id : Bool -> Bool = \x:Bool. x.

constT : Bool -> Bool = \_:Bool. T.

fty : Bool -> Type.
fty T = Bool -> Bool,
fty F = Bool -> Bool.

f : (x : Bool) -> fty x.
f T = id,
f F = constT.

main : TwoBools
  = TB (f T F) (f F F).
