-- vim: ft=idris

postulate Bool : *.
postulate T : Bool.
postulate F : Bool.

postulate Id : (a : *) -> (x : a) -> (y : a) -> *.
postulate Refl : (a : *) -> (x : a) -> Id x x.

not : Bool -> Bool.
  pat (x : Bool).
    not T = F,
    not F = T.

notnot : (x : Bool) -> Id x (not (not x)).
  pat (x : Bool).
    notnot T = Refl Bool T,
    notnot F = Refl Bool F.

main :R: Id F (not (not F)).
  main = notnot F.
