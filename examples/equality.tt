-- vim: ft=idris

postulate Bool : Type.
postulate T : Bool.
postulate F : Bool.

postulate Id : (a : Type) -> (x : a) -> (y : a) -> Type.
postulate Refl : (a : Type) -> (x : a) -> Id x x.

not : Bool -> Bool.
  not T = F,
  not F = T.

notnot : (x : Bool) -> Id x (not (not x)).
  notnot T = Refl Bool T,
  notnot F = Refl Bool F.

main :R: Id F (not (not F)).
  main = notnot F.
