-- vim: ft=ttstar
  let
    postulate True : Bool
    postulate False : Bool
    postulate Z : Nat
    postulate S : (_x0) -> Nat
    postulate Even : Tag
    postulate Odd : Tag
    fun 
       Even = (\n. 
        let f 
           Z = True
           (S m) = fun Odd m
        in f n)
       Odd = (\n. 
        let f 
           Z = False
           (S m) = fun Even m
        in f n)
    even = fun Even
    main = even 8
  in main
