-- vim: ft=ttstar
  let
    constructor Z : Nat
    constructor S : (x) -> Nat
    plus 
       Z m = m
       (S n) m = S (plus n m)
    const = (\x. (\y. x))
    main = 
      let
        apply 
           f Z = f Z
           f (S x) = f (S x)
        constructor Q : Nat
        three = 3
      in plus (apply (\x. x) 2) (apply (const Q) three)
  in main
