-- vim: ft=ttstar
  let
    constructor N : Type
    constructor Z : N
    constructor S : (x) -> N
    constructor Vec : (_x0) -> (_x1) -> Type
    constructor Nil : (a) -> Vec Z a
    constructor Cons : (a) -> (n) -> (x) -> (xs) -> Vec (S n) a
    plus 
       Z n = n
       (S m) n = S (plus m n)
    append 
       a {_} n (Nil [_]) ys = ys
       a ({_} m) n (Cons [_] [_] x xs) ys = Cons a (plus m n) x (append a m n xs ys)
    main = append N Z 1 (Nil N) (Cons N Z 4 (Nil N))
  in main
