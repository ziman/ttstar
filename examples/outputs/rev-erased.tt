-- vim: ft=ttstar
  let
    postulate T : Bool
    postulate F : Bool
    postulate Nil : List
    postulate Cons : (x) -> (xs) -> List
    postulate RNil : Rev
    postulate RSnoc : (x) -> (rxs) -> Rev
    rev' 
       rxs Nil = rxs
       rxs (Cons y ys) = rev' (RSnoc y rxs) ys
    rev = (\xs. rev' RNil xs)
    reverse' 
       RNil = Nil
       (RSnoc x rxs) = Cons x (reverse' rxs)
    reverse = (\xs. reverse' (rev xs))
    main = reverse (Cons T (Cons F (Cons T (Cons F Nil))))
  in main
