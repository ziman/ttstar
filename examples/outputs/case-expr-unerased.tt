  let
    postulate Bool : Type
    postulate T : Bool
    postulate F : Bool
    postulate Unit : Type
    postulate MkUnit : Unit
    not 
      [_] T = F
      [_] F = T
    retTy 
      [_] T = Bool
      [_] F = Unit
