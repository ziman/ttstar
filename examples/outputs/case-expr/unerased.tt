-- vim: ft=ttstar

  let
    constructor Bool : Type
    constructor T : Bool
    constructor F : Bool
    constructor Unit : Type
    constructor MkUnit : Unit
    not 
      not T = F
      not F = T
    retTy 
      retTy T = Bool
      retTy F = Unit
    main = not 
      let f 
        f F MkUnit = MkUnit
        f T T = F
        f T F = T
      in f (not F) T
  in main
