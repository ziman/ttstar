-- vim: ft=agda

postulate T : *.
postulate A : T.
postulate B : T.
postulate Op : (x :R: T) -> (y :R: T) -> T.

id : T -> T = \y : T. y.

constA : T -> T = \_ : T. A.

apply (f : T -> T) : T -> T
    = \x : T. f x.

test1 : T
    = [apply : (T -> T) -> T -> T] id B.

test2 : T
    = [apply : (T -> T) -> T -> T] constA B.

main :R: T
    = Op test1 test2.
