-- vim: ft=idris

data Bool : Type where
  T : Bool,
  F : Bool.

data Unit : Type where
  MkUnit : Unit.

not : Bool -> Bool.
not T = F,
not F = T.

retTy : Bool -> Type.
retTy T = Bool,
retTy F = Unit.

main : Bool
  = not
      case not F with
        f : (x : Bool) -> retTy x.
        f F = MkUnit,
        f T = F.
