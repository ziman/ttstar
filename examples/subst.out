### Desugared ###

Id : (A : *) -> (_ : A) -> (_ : A) -> *

Refl : (A : *) -> (x : A) -> (Id A x x)

subst : (A : *) -> (P : (_ : A) -> *) -> (x : A) -> (y : A) -> (eq : (Id A x y)) -> (px : (P x)) -> (P y)
subst = (\A : *. (\P : (_ : A) -> *. (\x : A. (\y : A. (\eq : (Id A x y). (\px : (P x). 
  case eq of
    Refl (a : *) (x : A) -> px))))))

